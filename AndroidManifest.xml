<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diarycall"
    android:versionCode="1"
    android:versionName="1.0" xmlns:tools="http://schemas.android.com/tools">

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" tools:ignore="OldTargetApi"/>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.diarycall.GUI.DiaryCall"
            android:label="@string/app_name" tools:ignore="InnerclassSeparator" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.diarycall.GUI.ContatoGUI" android:label="@string/app_name" tools:ignore="InnerclassSeparator"/>
        <activity android:name="com.example.diarycall.GUI.CallReceive" android:label="@string/app_name" tools:ignore="InnerclassSeparator"/>
        <activity android:name="com.example.diarycall.GUI.InfoContato" android:label="@string/app_name" tools:ignore="InnerclassSeparator"/>
        
    </application>

</manifest>
